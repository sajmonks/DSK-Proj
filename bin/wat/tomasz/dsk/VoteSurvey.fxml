<AnchorPane prefHeight="323.0" prefWidth="543.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="wat.tomasz.dsk.SurveyVoteController">
